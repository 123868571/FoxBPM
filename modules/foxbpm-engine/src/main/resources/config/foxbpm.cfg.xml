<?xml version="1.0" encoding="UTF-8"?>
<foxbpmconfig:FoxBPMConfig xmlns:foxbpmconfig="http://www.foxbpm.org/foxbpmconfig">
	<resourcePathConfig>

		<resourcePath id="foxbpmflowconnector" name="流程连接器路径"
			type="flowConnector" projectName="foxbpm-connector"
			virtualPath="src/main/java/org/foxbpm/connector/flowconnector/" src="org/foxbpm/connector/flowconnector/" />
		<resourcePath id="foxbpmactorconnector" name="处理者选择器路径"
			type="actorConnector" projectName="foxbpm-connector"
			virtualPath="src/main/java/org/foxbpm/connector/actorconnector/" src="org/foxbpm/connector/actorconnector/" />

		<resourcePath id="flowconnector2" name="流程连接器路径" type="flowConnector"
			projectName="connector2" virtualPath="src/main/java/connector/flowconnector/"
			src="connector/flowconnector/" />
		<resourcePath id="actorconnector2" name="处理者选择器路径" type="actorConnector"
			projectName="connector2" virtualPath="src/main/java/connector/actorconnector/"
			src="connector/actorconnector/" />

	</resourcePathConfig>
	<scriptLanguageConfig selected="Groovy">
		<scriptLanguage id="BeanShell"
			classImpl="org.foxbpm.engine.impl.scriptlanguage.BeanShellScriptLanguageMgmtImpl" />
		<scriptLanguage id="Groovy"
			classImpl="org.foxbpm.engine.impl.scriptlanguage.GroovyScriptLanguageMgmtImpl" />
	</scriptLanguageConfig>

	<taskCommandConfig>
		<taskCommandDefinition id="general" name="通用"
			isEnabled="true" type="toDoTasks"
			command="org.foxbpm.engine.impl.task.command.GeneralTaskCommand" cmd="org.foxbpm.engine.impl.task.cmd.CompleteGeneralTaskCmd"
			filter="org.foxbpm.engine.impl.task.filter.CompleteGeneralTaskFilter" />
		<taskCommandDefinition id="startandsubmit" name="启动并提交"
			command="org.foxbpm.engine.impl.task.command.StartAndSubmitTaskCommand"
			cmd="org.foxbpm.engine.impl.task.cmd.StartAndSubmitTaskCmd"
			isEnabled="true" type="toDoTasks"
			filter="org.foxbpm.engine.impl.task.filter.StartAndSubmitTaskFilter" />
		<taskCommandDefinition id="submit" name="提交"
			command="org.foxbpm.engine.impl.task.command.SubmitTaskCommand" cmd="org.foxbpm.engine.impl.task.cmd.SubmitTaskCmd"
			isEnabled="true" type="toDoTasks"
			filter="org.foxbpm.engine.impl.task.filter.SubmitTaskFilter" />
		<taskCommandDefinition id="claim" name="接收任务"
			command="org.foxbpm.engine.impl.task.command.ClaimTaskCommand" cmd="org.foxbpm.engine.impl.task.cmd.ClaimTaskCmd"
			isEnabled="true" type="sharedTasks"
			filter="org.foxbpm.engine.impl.task.filter.ClaimTaskFilter" />
		<taskCommandDefinition id="releaseTask" name="释放任务"
			command="org.foxbpm.engine.impl.task.command.ReleaseTaskCommand" cmd="org.foxbpm.engine.impl.task.cmd.ReleaseTaskCmd"
			isEnabled="true" type="toDoTasks"
			filter="org.foxbpm.engine.impl.task.filter.ReleaseTaskFilter" />
		<taskCommandDefinition id="saveTaskDraft" name="保存草稿"
			command="org.foxbpm.engine.impl.task.command.SaveTaskDraftCommand"
			cmd="org.foxbpm.engine.impl.task.cmd.SaveTaskDraftCmd" isEnabled="true"
			type="toDoTasks" filter="org.foxbpm.engine.impl.task.filter.SaveTaskDraftFilter" />
		<taskCommandDefinition id="processStatus" name="流程状态"
			isEnabled="true" type="all" />

	</taskCommandConfig>

	<sysMailConfig selected="方正国际邮箱">
		<mailInfo mailAddress="jiang_nan@founder.com" mailName="方正国际邮箱"
			smtpHost="mail.founder.com.cn" smtpPort="25" userName="kenshin"
			password="kenshin" />
	</sysMailConfig>

	<connectionManagementConfig selected="General">
		<connectionManagement id="General" name="通用连接管理"
			classImpl="com.founder.fix.fixflow.expand.database.GeneralConnectionManagement" />
	</connectionManagementConfig>
	<eventListenerConfig>
		<eventListener eventType="process-start"
			listenerClass="org.foxbpm.engine.impl.listener.runningtrack.StartEventTrackListener" />
		<eventListener eventType="node-enter"
			listenerClass="org.foxbpm.engine.impl.listener.runningtrack.FlowNodeTrackListener" />
		<eventListener eventType="node-execute"
			listenerClass="org.foxbpm.engine.impl.listener.runningtrack.FlowNodeTrackListener" />
		<eventListener eventType="node-leave"
			listenerClass="org.foxbpm.engine.impl.listener.runningtrack.FlowNodeTrackListener" />
		<eventListener eventType="sequenceflow-take"
			listenerClass="org.foxbpm.engine.impl.listener.runningtrack.SequenceTrackEventListener" />
		<eventListener eventType="process-start"
			listenerClass="org.foxbpm.engine.impl.listener.task.StartEventTaskListener" />
		<eventListener eventType="process-end"
			listenerClass="org.foxbpm.engine.impl.listener.task.EndEventTaskListener" />
	</eventListenerConfig>
</foxbpmconfig:FoxBPMConfig>
